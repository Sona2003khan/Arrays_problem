public class ArraysEasy5 {
    public static void rotate_one_plave(int nums[]){

        int n=nums.length;
        int temp=nums[0];
        for(int i=1;i<n;i++){
            nums[i-1]=nums[i];
        }
        nums[n-1]=temp;
        
    }
    public static void main(String[] args) {
        int nums[]={1,2,3,4,5};
        rotate_one_plave(nums);
        for(int i=0;i<nums.length;i++){
            System.out.print(nums[i]+" ");
        }
    }
}

