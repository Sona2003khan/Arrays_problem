import java.util.*;
public class ArraysEasy3 {
    public static boolean if_array_sorted(int nums[]){
        int n=nums.length;
        for(int i=0;i<n-1;i++){
            if(nums[i]>nums[i+1]){
                return false;        
            }
        }
        return true;

    }
    public static void main (String args[]){
        int nums[]={2,4,7,9,10};
        boolean result=if_array_sorted(nums);
        System.out.println(result);
    }
}
